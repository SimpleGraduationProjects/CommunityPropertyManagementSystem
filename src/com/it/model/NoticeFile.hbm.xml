<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.it.model.NoticeFile" table="t_NoticeFile">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="s_0" column="f_uploadName" />
		<property name="s_1" column="f_fileName" />
		<property name="s_2" column="f_uploadTime" />
		<property name="s_3" column="n_name" />
		<property name="s_4" column="n_type" />
		<property name="s_5" column="n_info" />
		<property name="s_6" column="n_bz" />
		<property name="s_7" column="f_status" />
	</class>
</hibernate-mapping>